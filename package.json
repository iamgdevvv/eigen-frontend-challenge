		"@fontsource-variable/inter": "^5.2.6",
		"@react-router/fs-routes": "^7.8.0",
		"@react-router/node": "^7.8.0",
		"@react-router/serve": "^7.8.0",
		"antd": "^5.26.7",
		"clsx": "^2.1.1",
		"dayjs": "^1.11.13",
		"isbot": "^5.1.29",
		"lru-cache": "^11.1.0",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-router": "^7.8.0",
		"tailwind-merge": "^3.3.1"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.32.0",
		"@react-router/dev": "^7.8.0",
		"@tailwindcss/vite": "^4.1.11",
		"@types/node": "^22",
