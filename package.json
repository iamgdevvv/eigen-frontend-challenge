		"prettier-plugin-classnames": "^0.8.2",
		"prettier-plugin-merge": "^0.8.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"sharp": "^0.34.3",
		"svgo": "^4.0.0",
		"tailwindcss": "^4.1.11",
