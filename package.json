{
	"name": "eigen-frontend-challenge",
	"version": "1.0.0",
	"description": "This project is a submission for the Eigen Frontend Challenge",
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"check:unused": "knip",
		"clean": "rm -rf node_modules",
